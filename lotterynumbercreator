import random as rd
import time

lottery = []
while len(lottery) != 6:
    passthisno = 0
    no = rd.randrange(1,46,1)
    for i in lottery:
        if no == i:
            passthisno = 1
    if passthisno == 0:
        lottery.append(no)
        
for i in lottery:
    print(i)
    time.sleep(0.5)
